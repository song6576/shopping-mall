 <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.row)\"\n            >删除</el-button\n          >\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      class=\"pager\"\n      layout=\"prev, pager, next\"\n      :total=\"config.total\"\n      :current-page.sync=\"config.page\"\n      @current-change=\"changePage\"\n      :page-size=\"20\"\n    >\n    </el-pagination>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    tableData: Array,\n    tableLabel: Array,\n    config: Object,\n  },\n  methods: {\n    handleEdit(row) {\n      this.$emit(\"edit\", row);\n    },\n    handleDelete(row) {\n      this.$emit(\"del\", row);\n    },\n    changePage(page) {\n      this.$emit(\"changePage\", page);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.common-table {\n  height: calc(100% - 62px);\n  background-color: #fff;\n  position: relative;\n  .pager {\n    position: absolute;\n    bottom: 0;\n    right: 20px;\n  }\n}\n</style>\n"]}]}